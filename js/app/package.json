{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "PORT=3011 REACT_APP_GRAPHQL_ENDPOINT=http://localhost:3010/graphql craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"{src,public,typings}/**/*.{ts,tsx,js,jsx,html,json,css,scss}\"",
    "download-schema": "apollo client:download-schema --endpoint=http://localhost:3010/graphql",
    "types": "apollo client:codegen --localSchemaFile=schema.json --target=typescript --no-addTypename --outputFlat src/generated/gqlTypes.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@craco/craco": "^6.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/codemirror": "^5.60.1",
    "@types/jest": "^26.0.15",
    "@types/mousetrap": "^1.6.8",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "classnames": "^2.3.1",
    "codemirror": "^5.62.0",
    "graphql": "15.5.2",
    "highlight.js": "^11.2.0",
    "mousetrap": "^1.6.5",
    "node-sass": "4.14.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "rehype-highlight": "^5.0.0",
    "rehype-react": "^6.2.1",
    "rehype-stringify": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-wiki-link": "^1.0.3",
    "typescript": "^4.1.2",
    "unified": "^9.2.1",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "apollo": "^2.33.6",
    "autoprefixer": "^9",
    "postcss": "^7",
    "prettier": "^2.3.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "resolutions": {
    "ts-invariant": "^0.5.0",
    "//": "https://github.com/syntax-tree/hast-util-to-text/issues/4",
    "hast-util-to-text": "3.0.0"
  }
}
